<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cookExperimentFindReceipeVM" type="com.altlokaltsapplication.app.modules.cookexperimentfindreceipe.data.viewmodel.CookExperimentFindReceipeVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearCookExperiment"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylegray_50">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_342pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
        </androidx.appcompat.widget.Toolbar>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtWhatingredient"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.0"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:text="@{cookExperimentFindReceipeVM.cookExperimentFindReceipeModel.txtWhatingredient}"
            style="@style/txtPoppinsbold24" tools:text="@string/msg_what_ingredient"/>
        <LinearLayout android:id="@+id/linearItem"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
            <TextView android:ellipsize="end"
                android:id="@+id/txtIngridientsfor"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_weight="0.71" android:layout_width="0dp"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:singleLine="true"
                android:text="@{cookExperimentFindReceipeVM.cookExperimentFindReceipeModel.txtIngridientsfor}"
                style="@style/txtPoppinsregular16" tools:text="@string/msg_ingridients_for"/>
            <LinearLayout android:id="@+id/linearForm"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_64pxh"
                android:layout_weight="0.29" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStyleteal_1">
                <ImageView android:id="@+id/imageGrid"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_grid_6x6"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_grid_6x6"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAdd"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.74"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{cookExperimentFindReceipeVM.cookExperimentFindReceipeModel.txtAdd}"
                    style="@style/txtPoppinsbold16_2" tools:text="@string/lbl_add"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearItem1"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_52cornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtMilkCounter"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_weight="0.61" android:layout_width="0dp"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:singleLine="true"
                android:text="@{cookExperimentFindReceipeVM.cookExperimentFindReceipeModel.txtMilkCounter}"
                style="@style/txtPoppinsbold16_1" tools:text="@string/lbl_1_milk"/>
            <FrameLayout android:id="@+id/frameIcon"
                android:layout_height="@dimen/_32pxv"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_229pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="@dimen/_32pxh" style="@style/groupStylewhite_1">
                <ImageView android:id="@+id/imagePlus"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginHorizontal="@dimen/_10pxh"
                    android:layout_marginVertical="@dimen/_10pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_plus_12x12"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_plus_12x12"/>
            </FrameLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearItem2"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_52cornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtChickenCounter"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_weight="0.73" android:layout_width="0dp"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:singleLine="true"
                android:text="@{cookExperimentFindReceipeVM.cookExperimentFindReceipeModel.txtChickenCounter}"
                style="@style/txtPoppinsbold16_1" tools:text="@string/lbl_2_chicken"/>
            <FrameLayout android:id="@+id/frameIcon1"
                android:layout_height="@dimen/_32pxv"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_193pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="@dimen/_32pxh" style="@style/groupStylewhite_1">
                <ImageView android:id="@+id/imagePlusOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginHorizontal="@dimen/_10pxh"
                    android:layout_marginVertical="@dimen/_10pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_plus_12x12"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_plus_12x12"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackpotatocounter"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_93pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_343pxh">
            <LinearLayout android:id="@+id/linearItem3"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_52cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPotatoCounter"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_weight="0.69"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{cookExperimentFindReceipeVM.cookExperimentFindReceipeModel.txtPotatoCounter}"
                    style="@style/txtPoppinsbold16_1" tools:text="@string/lbl_3_potato"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_radius_4"
                    android:id="@+id/btnPlusTwo"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_206pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="@dimen/_32pxh"
                    android:paddingHorizontal="@dimen/_10pxh"
                    android:paddingVertical="@dimen/_10pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_plus_12x12" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnFindRecipe"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.0"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_find_recipe"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_find_recipe"/>
        </FrameLayout>
    </LinearLayout>
</layout>
