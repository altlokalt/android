<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cookExperimentFitsReceipeVM" type="com.altlokaltsapplication.app.modules.cookexperimentfitsreceipe.data.viewmodel.CookExperimentFitsReceipeVM"/>
    </data>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearCookExperiment"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearNavbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_50">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCookExperiment"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_117pxh"
                    android:layout_marginStart="@dimen/_84pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.93"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{cookExperimentFitsReceipeVM.cookExperimentFitsReceipeModel.txtCookExperiment}"
                    style="@style/txtPoppinsbold16_1" tools:text="@string/lbl_cook_experiment"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <HorizontalScrollView android:fillViewport="true"
            android:id="@+id/horizontalScrollHorizontalscrollmilk"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:scrollbars="none">
            <LinearLayout android:id="@+id/linearTabs"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearTab"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.19"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylegray_52cornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMilk"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_weight="-0.13"
                        android:layout_width="0dp"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_8pxh"
                        android:singleLine="true"
                        android:text="@{cookExperimentFitsReceipeVM.cookExperimentFitsReceipeModel.txtMilk}"
                        style="@style/txtPoppinsbold16_1" tools:text="@string/lbl_milk"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAdd"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginStart="@dimen/_311pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="-0.13"
                        android:layout_width="0dp"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@{cookExperimentFitsReceipeVM.cookExperimentFitsReceipeModel.txtAdd}"
                        style="@style/txtPoppinsbold16_2" tools:text="@string/lbl_add"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnChicken"
                    android:layout_height="@dimen/_37pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_101pxh"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_chicken"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_3" tools:text="@string/lbl_chicken"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnPotatoes"
                    android:layout_height="@dimen/_37pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_106pxh"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_potatoes"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_3" tools:text="@string/lbl_potatoes"/>
                <FrameLayout android:id="@+id/frameForm"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="@dimen/_82pxh" style="@style/groupStyleteal_1">
                    <ImageView android:id="@+id/imageGrid"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginHorizontal="@dimen/_14pxh"
                        android:layout_marginVertical="@dimen/_14pxv"
                        android:layout_width="@dimen/_12pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_grid_6x6"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_grid_6x6"/>
                </FrameLayout>
            </LinearLayout>
        </HorizontalScrollView>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearResult"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_247pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_29pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius_1">
            <LinearLayout android:id="@+id/linearRow23recipeyouc"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <AutoCompleteTextView android:completionThreshold="1"
                    android:ellipsize="end"
                    android:fontFamily="@font/poppins"
                    android:hint="@string/lbl_fix"
                    android:id="@+id/autoCompTxtRecipey"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@string/msg_23_recipe_you_c"
                    android:textColor="@color/bluegray_900"
                    android:textColorHint="@color/black_900"
                    android:textFontWeight="700"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="bold" tools:text="@string/msg_23_recipe_you_c"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFilter"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_101pxh"
                    android:layout_weight="0.16"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{cookExperimentFitsReceipeVM.cookExperimentFitsReceipeModel.txtFilter}"
                    style="@style/txtPoppinsregular16_3" tools:text="@string/lbl_filter"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearItem"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_52cornerRadius">
                <View
                    android:background="@drawable/rectangle_bg_gray_300_radius_8"
                    android:id="@+id/viewImage"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_40pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.85"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{cookExperimentFitsReceipeVM.cookExperimentFitsReceipeModel.txtLanguage}"
                    style="@style/txtPoppinsbold16_1" tools:text="@string/msg_italian_spaghet"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearItem1"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_52cornerRadius">
                <View
                    android:background="@drawable/rectangle_bg_gray_300_radius_8"
                    android:id="@+id/viewImageOne"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_40pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguageOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.85"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{cookExperimentFitsReceipeVM.cookExperimentFitsReceipeModel.txtLanguageOne}"
                    style="@style/txtPoppinsbold16_1" tools:text="@string/msg_italian_spaghet"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearItem2"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_52cornerRadius">
                <View
                    android:background="@drawable/rectangle_bg_gray_300_radius_8"
                    android:id="@+id/viewImageTwo"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_40pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguageTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.85"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{cookExperimentFitsReceipeVM.cookExperimentFitsReceipeModel.txtLanguageTwo}"
                    style="@style/txtPoppinsbold16_1" tools:text="@string/msg_italian_spaghet"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearItem3"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_52cornerRadius">
                <View
                    android:background="@drawable/rectangle_bg_gray_300_radius_8"
                    android:id="@+id/viewImageThree"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_40pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguageThree"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.85"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{cookExperimentFitsReceipeVM.cookExperimentFitsReceipeModel.txtLanguageThree}"
                    style="@style/txtPoppinsbold16_1" tools:text="@string/msg_italian_spaghet"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
