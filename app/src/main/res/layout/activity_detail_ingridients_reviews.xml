<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="detailIngridientsReviewsVM" type="com.altlokaltsapplication.app.modules.detailingridientsreviews.data.viewmodel.DetailIngridientsReviewsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearDetailIngridie"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylegray_50">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_weight="0.76"
                    android:layout_width="0dp"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{detailIngridientsReviewsVM.detailIngridientsReviewsModel.txtLanguage}"
                    style="@style/txtPoppinsbold16_1" tools:text="@string/msg_italian_spaghet2"/>
                <ImageView android:id="@+id/imageFavorite"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_favorite_18x20"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_favorite_18x20"/>
                <ImageView android:id="@+id/imagePlus"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_plus"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_plus"/>
                <ImageView android:id="@+id/imageUpload"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_upload"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_upload"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearTabs"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTab"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.42" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtIngridients"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{detailIngridientsReviewsVM.detailIngridientsReviewsModel.txtIngridients}"
                    style="@style/txtPoppinsregular16_1" tools:text="@string/lbl_ingridients"/>
                <View android:background="@color/gray_50"
                    android:id="@+id/lineLabel"
                    android:layout_height="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_96pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTab1"
                android:layout_height="wrap_content"
                android:layout_weight="0.37" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDirections"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{detailIngridientsReviewsVM.detailIngridientsReviewsModel.txtDirections}"
                    style="@style/txtPoppinsregular16_1" tools:text="@string/lbl_directions"/>
                <View android:background="@color/gray_50"
                    android:id="@+id/lineLabelOne"
                    android:layout_height="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_80pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTab2"
                android:layout_height="wrap_content"
                android:layout_weight="0.21" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtReview"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{detailIngridientsReviewsVM.detailIngridientsReviewsModel.txtReview}"
                    style="@style/txtPoppinsbold16_3" tools:text="@string/lbl_review"/>
                <View android:background="@color/gray_900"
                    android:id="@+id/lineLabelTwo"
                    android:layout_height="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_64pxh"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackloremipsumdol"
            android:layout_height="@dimen/_354pxv"
            android:layout_marginBottom="@dimen/_291pxv"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearReviews"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="match_parent" android:orientation="vertical">
                <View android:background="@color/gray_300"
                    android:id="@+id/viewImage"
                    android:layout_height="@dimen/_143pxv" android:layout_width="@dimen/_343pxh"/>
                <TextView android:id="@+id/txtLoremipsumdol"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:text="@{detailIngridientsReviewsVM.detailIngridientsReviewsModel.txtLoremipsumdol}"
                    style="@style/txtPoppinsregular16_3" tools:text="@string/msg_lorem_ipsum_dol3"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearPersona"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_gray_300_radius_16"
                        android:id="@+id/viewAvatar"
                        android:layout_height="@dimen/_32pxh" android:layout_width="@dimen/_32pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtShooPharDhie"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:letterSpacing="0.0"
                        android:lineSpacingExtra="@dimen/_8pxh"
                        android:singleLine="true"
                        android:text="@{detailIngridientsReviewsVM.detailIngridientsReviewsModel.txtShooPharDhie}"
                        style="@style/txtPoppinsbold16_1" tools:text="@string/lbl_shoo_phar_dhie"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearMetadata"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDuration"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.64"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{detailIngridientsReviewsVM.detailIngridientsReviewsModel.txtDuration}"
                        style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_2_hours_ago"/>
                    <LinearLayout
                        android:id="@+id/linearRowtwohundredtwelve"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_228pxh"
                        android:layout_weight="0.36"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTwoHundredTwelve"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.61"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{detailIngridientsReviewsVM.detailIngridientsReviewsModel.txtTwoHundredTwelve}"
                            style="@style/txtPoppinssemibold12_1" tools:text="@string/lbl_212"/>
                        <ImageView android:id="@+id/imageAirplane"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_12pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_airplane"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_airplane"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:hint="@string/lbl_editabletext"
                android:id="@+id/textInputLayTheRecipesog"
                android:layout_gravity="bottom"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_weight="0.68" android:layout_width="0dp"
                android:textColorHint="@color/black_900" style="@style/txtSolid">
                <com.google.android.material.textfield.TextInputEditText
                    android:ellipsize="end"
                    android:fontFamily="@font/poppins"
                    android:id="@+id/etTheRecipesog"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.0"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@={detailIngridientsReviewsVM.detailIngridientsReviewsModel.etTheRecipesogValue}"
                    android:textColor="@color/bluegray_900"
                    android:textFontWeight="400"
                    android:textSize="@dimen/_16pxh"
                    android:textStyle="normal" tools:text="@string/msg_the_recipe_so_g"/>
            </com.google.android.material.textfield.TextInputLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
